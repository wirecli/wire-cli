{
  "name": "wirecli/wire-cli",
  "description": "An extendable ProcessWire command line interface",
  "keywords": [
    "processwire"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Joani Eizmendi",
      "homepage": "https://github.com/flydev-fr"
    },
    {
      "name": "Marcus Herrmann",
      "homepage": "https://bigger-on-the-inside.net"
    },
    {
      "name": "Hari K T",
      "homepage": "http://harikt.com"
    },
    {
      "name": "Bea David",
      "homepage": "http://justonestep.de/"
    },
    {
      "name": "Camilo Castro",
      "homepage": "http://www.cervezapps.cl"
    }
  ],
  "bin": [
    "wirecli"
  ],
  "autoload": {
    "psr-4": {
      "Wirecli\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.8",
    "monolog/monolog": "^3.5",
    "pmjones/auto-shell": "^1.0.1",
    "symfony/console": "^6.4",
    "symfony/filesystem": "^6.4",
    "symfony/process": "^6.4",
    "ext-pdo_mysql": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-iconv": "*",
    "ext-ctype": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.1.0"
    }
  },
  "version": "1.4.12",
  "extra": {
    "processwire": {
      "min-version": "3.0.0",
      "max-version": "4.0.0"
    }
  }
}
